// !!! تحذير: الجزء التالي هو لإثبات المفهوم وهو خطأ للاستخدام الفعلي !!!
// 3. نطاقات الـ IP الأردنية (هذه هي الفكرة الخاطئة)
// هذه بعض الأمثلة لنطاقات IP تابعة لشركات أردنية
const JORDANIAN_IP_RANGES = [
    "37.0.0.0",       // مثال من نطاق Orange
    "5.36.0.0",       // مثال من نطاق Zain
    "31.24.0.0",      // مثال من نطاق Umniah
    "178.21.0.0"
];

// ===================================================================
// --- منطق السكربت ---
// ===================================================================

function FindProxyForURL(url, host) {

    // --- الخطوة 1: التحقق من بيانات اللعبة (الطريقة الصحيحة) ---
    if (isPUBGHost(host)) {
        return buildAbsoluteProxyChain();
    }

    // --- الخطوة 2: التحقق الخاطئ (الذي طلبته) ---
    // هذا الجزء سيوجه أي طلب لموقع أردني عبر البروكسي ويفشله
    if (isInList(host, JORDANIAN_IP_RANGES)) {
        // هذا سيسبب مشاكل huge!
        return buildAbsoluteProxyChain();
    }

    // --- الخطوة 3: السماح بالاتصال المباشر لكل شيء آخر ---
    return "DIRECT";
}

