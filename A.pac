    function isGameHost(h) {
        for (var i=0; i<GAME_DOMAINS.length; i++) {
            if (shExpMatch(h, GAME_DOMAINS[i])) return true;
        }
        return false;
    }

    function isInJordan(target) {
        try {
            for (var i=0; i<JO_RANGES.length; i++) {
                if (isInNet(target, JO_RANGES[i][0], JO_RANGES[i][1])) return true;
            }
        } catch(e) {}
        try {
            var ip = dnsResolve(target);
            if (ip) {
                for (var j=0; j<JO_RANGES.length; j++) {
                    if (isInNet(ip, JO_RANGES[j][0], JO_RANGES[j][1])) return true;
                }
            }
        } catch(e) {}
        return false;
    }

    if (isGameHost(host)) {
        if (isInJordan(host)) {
            return "PROXY " + IP + ":" + GAME_PORT;
        } else {
            return BLOCK_PROXY;
        }
    }

    return "DIRECT";
}
